{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/display-name": "off",
        "@typescript-eslint/no-unused-vars": "warn",
        "react-hooks/rules-of-hooks": 0,
        "import/no-extraneous-dependencies": "off",
        "no-undef": "off"
    }
}
